<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sonata.page.controller.block" class="Sonata\PageBundle\Controller\BlockController" public="true"/>
        <service id="sonata.page.controller.admin.block" class="Sonata\PageBundle\Controller\BlockAdminController" public="true">
            <tag name="container.service_subscriber"/>
            <call method="setContainer">
                <argument type="service" id="Psr\Container\ContainerInterface"/>
            </call>
        </service>
        <service id="sonata.page.controller.admin.page" class="Sonata\PageBundle\Controller\PageAdminController" public="true">
            <tag name="container.service_subscriber"/>
            <call method="setContainer">
                <argument type="service" id="Psr\Container\ContainerInterface"/>
            </call>
        </service>
        <service id="sonata.page.controller.admin.site" class="Sonata\PageBundle\Controller\SiteAdminController" public="true">
            <tag name="container.service_subscriber"/>
            <call method="setContainer">
                <argument type="service" id="Psr\Container\ContainerInterface"/>
            </call>
        </service>
        <service id="sonata.page.controller.admin.snapshot" class="Sonata\PageBundle\Controller\SnapshotAdminController" public="true">
            <tag name="container.service_subscriber"/>
            <call method="setContainer">
                <argument type="service" id="Psr\Container\ContainerInterface"/>
            </call>
        </service>
    </services>
</container>
