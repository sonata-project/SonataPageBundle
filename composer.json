{
    "name": "sonata-project/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataPageBundle",
    "keywords": ["page", "sonata", "cms"],
    "homepage": "http://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.2",
        "sonata-project/admin-bundle": "~2.2,>=2.2.9",
        "sonata-project/seo-bundle": "~1.1",
        "sonata-project/doctrine-extensions" : "~1.0",
        "sonata-project/cache-bundle": "~2.1,>=2.1.7",
        "sonata-project/core-bundle": "~2.2@dev,>2.2.5",
        "symfony-cmf/routing-bundle": "~1.1",
        "sonata-project/notification-bundle": "~2.2",
        "sonata-project/datagrid-bundle": "~2.2@dev",

        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "require-dev": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2"
    },
    "autoload": {
        "psr-4": { "Sonata\\PageBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
