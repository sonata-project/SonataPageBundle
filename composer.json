{
    "name": "sonata-project/page-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a Site and Page management through container and block services",
    "keywords": [
        "page",
        "sonata",
        "cms"
    ],
    "homepage": "https://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-bundle": "^1.12.3 || ^2.0",
        "doctrine/persistence": "^1.3.3",
        "sonata-project/admin-bundle": "^3.35",
        "sonata-project/block-bundle": "^3.18",
        "sonata-project/cache": "^1.0.2 || ^2.0",
        "sonata-project/cache-bundle": "^2.4",
        "sonata-project/core-bundle": "^3.14",
        "sonata-project/datagrid-bundle": "^2.3",
        "sonata-project/doctrine-extensions": "^1.1.3",
        "sonata-project/doctrine-orm-admin-bundle": "^3.5",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/notification-bundle": "^3.3",
        "sonata-project/seo-bundle": "^2.5",
        "symfony-cmf/routing-bundle": "^1.1 || ^2.0",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/debug": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/form": "^4.3",
        "symfony/http-foundation": "^4.3.8",
        "symfony/http-kernel": "^4.3",
        "symfony/options-resolver": "^4.3",
        "symfony/process": "^4.3",
        "symfony/routing": "^4.3",
        "symfony/security-core": "^4.3",
        "symfony/security-http": "^4.3",
        "symfony/templating": "^4.3",
        "symfony/validator": "^4.3",
        "twig/twig": "^2.10"
    },
    "conflict": {
        "friendsofsymfony/rest-bundle": "<2.1 || >=3.0",
        "jms/serializer": "<0.13"
    },
    "require-dev": {
        "friendsofsymfony/rest-bundle": "^2.1",
        "jms/serializer-bundle": "^1.0 || ^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "nelmio/api-doc-bundle": "^2.4",
        "symfony/browser-kit": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\PageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\PageBundle\\Tests\\": "tests/"
        }
    }
}
